{
  "name": "@gymspace/sdk",
  "version": "1.0.2",
  "description": "GymSpace TypeScript SDK for API integration",
  "author": "GymSpace Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gymspace/gymspace.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/gymspace/gymspace/tree/main/packages/sdk",
  "bugs": {
    "url": "https://github.com/gymspace/gymspace/issues"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "postinstall": "pnpm run build"
  },
  "dependencies": {
    "@gymspace/shared": "^1.0.0",
    "axios": "^1.6.5",
    "zod": "^3.25.76"
  },
"devDependencies": {
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20.19.9",
    "@vitest/ui": "^2.0.5",
    "rimraf": "^5.0.5",
    "tsup": "^8.5.0",
    "typescript": "^5.7.2",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "axios": ">=1.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "gymspace",
    "sdk",
    "api",
    "client",
    "typescript",
    "gym",
    "management",
    "fitness"
  ]
}
